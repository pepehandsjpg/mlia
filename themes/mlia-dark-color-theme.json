{
  "name": "mlia-dark",
  "type": "dark",
  "background": "#171717",
  "foreground": "#EBEBEB",
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#BF616A"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "name": "Global definitions",
      "scope": "entity.name",
      "settings": {
        "foreground": "#5596b3"
      }
    },
    {
      "name": "Support Functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#5596b3",
      },
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#BF616A"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": [
        "meta.diff.range",
        "meta.diff.index",
        "meta.separator"
      ],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#434343"
      }
    }
  ],
